<div class="thumbnailcell spec_column_<%= column_number.to_s %>">
	<div class="thumbnail_container">
	  <% if Session.amazon? %>
       <%= product_image(thumbnails, :small) %>
    <% else %>
		  <%= link_to product_image(thumbnails, :small), TextSpec.cacheone(thumbnails.id, "productUrl#{fr?}"), {:class => 'easylink'} %>
		<% end %>
  </div>
  <%= render partial: "compare/product_info", locals: {product: thumbnails} %>
</div>
