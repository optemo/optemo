<!-- Piwik (original) -->
<script type="text/javascript">
var siteid = (document.title).search(/camera/i)==-1 ? 3 : 5;
var pkBaseURL = (("https:" == document.location.protocol) ? "https://pj.laserprinterhub.com/" : "http://pj.laserprinterhub.com/");
document.write(unescape("%3Cscript src='" + pkBaseURL + "piwik.js' type='text/javascript'%3E%3C/script%3E"));
</script><script type="text/javascript">
try {
var piwikTracker = Piwik.getTracker(pkBaseURL + "piwik.php", siteid);
piwikTracker.trackPageView();
piwikTracker.enableLinkTracking();
} catch( err ) {}
</script><noscript><p><img src="http://pj.laserprinterhub.com/piwik.php?idsite=1" style="border:0" alt=""/></p></noscript>
<!-- End Piwik Tag -->

