<% mb = main_boxes #Calculate this before navtitle %>
<%= render partial: "pagination" %>
<ul id="sorting_method_box"><li><strong><%= t("products.sortby") %>: </strong></li>
  <%=raw sortby %>
</ul>
<%unless (Session.search.collation.nil? || Session.search.col_emp_result) %>
  <div id="spellcheck_suggestion_box">
    <%if (Session.search.num_result==0)%>
      Showing results for <b><%=Session.search.collation%></b> as there were no results for <b><%=Session.search.keyword_search%></b>
    <%else%>
      <span style="color:black;">Did you mean</span> <a class="suggestion" href="#"> <%=Session.search.collation%></a><span style="color:black;">?</span>
    <%end%>
  </div>		
<%elsif (Session.search.num_result == 0 && (Session.search.col_emp_result || Session.search.collation==nil))%>
<div id="spellcheck_suggestion_box">
 Your search, <b><%=Session.search.keyword_search%></b>, did not match any products. 	
</div>
<%end%>
<div id="product_content">
    <div class='border-content'>	
    <%= raw mb %>
    <span id="actioncount" style="display:none"><%= [Session.search.id.to_s].pack("m").chomp %></span>
    </div>
    <%= render partial: "pagination", locals: {bottom: true} %>
</div>
<div style="clear:both;"></div>
