<% mb = main_boxes #Calculate this before navtitle %>
<%= render partial: "pagination" %>
<ul id="sorting_method_box"><li><strong><%= t("products.sortby") %>: </strong></li>
  <%=raw sortby %>
</ul>
<%if (Session.search.collation!=nil)%>
  <div id="spellcheck_suggestion_box">
    You searched for <b><%=Session.search.keyword%></b>,
	,did you mean
    <b><%if (Session.search.sc_emp_result)%>
      <%=Session.search.collation%>
    <%else%>
      <a class="suggestion" href="#"> <%=Session.search.collation%></a>
    <%end%></b>
  </div>		
<%end%>
<div id="product_content">
    <div class='border-content'>	
    <%= raw mb %>
    <span id="actioncount" style="display:none"><%= [Session.search.id.to_s].pack("m").chomp %></span>
    </div>
    <%= render partial: "pagination", locals: {bottom: true} %>
</div>
<div style="clear:both;"></div>
