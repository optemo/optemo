	<span style="color:red;" id="error"><%= flash[:error] unless flash[:error].nil? %></span>
	<% if flash[:notice] %>
	<span class="notice"><%= flash[:notice] %></span>
	<% end %>
	<div class='<%= s.lineItemView ? "lv" : "gv" %><%= " sv" if s.directLayout %><%= " sv_ungrouped" if @groupings.nil? && s.directLayout %>'>
	<div id="navigator_bar" class="rounded_nav rounded">
		<div class="tl"></div><div class="tr"></div>
	    <%= navtitle %>
	<% if s.lineItemView %>
		<% columntext = columntext(@groupings) %>
	<div id="columnheader">
		<div id="firstcolumn">
			<%= columntext[0] %>
		</div>
		<div id="secondcolumn">
			<%= columntext[1] %>
		</div>
		<div id="thirdcolumn">
			<%= columntext[2] %>
		</div>
	</div>
	<% end %>
		<div class="bl"></div><div class="br"></div>
	<%= '</div>' if !s.lineItemView %>
	<% if s.directLayout %>
		<% if @groupings.nil? %>
			<% @products.each_index do |i| %>
				<%= render :partial => 'singlelist', :locals => {:product => @products[i], :i => i, :s => s} %>
			<% end %>
		<% else %>
			<% @groupings.each_pair do |feature, products| %>
				<%= render :partial => 'grouping', :locals => {:feat => feature, :cheapest => products[0], :best => products[1], :group => true, :s => s}%>
			<% end %>
		<% end %>
	<% else %>
		<% for i in 0..[s.search.cluster_count, s.numGroups].min-1 %>
		  <% if i % (Float(s.numGroups)/3).ceil == 0 && !s.lineItemView %>
			<div class="rowdiv">
			<% open = true %>
		  <% end %>
		  <!-- Navbox partial to draw boxes -->
		  <%= render :partial => (s.lineItemView ? 'listbox' : 'navbox'), :locals => {:i => i, :cluster => s.search.clusters[i] , :group => s.search.clusters[i].size>1, :product => s.search.clusters[i].representative, :s => s} %>
		  <% if i % (Float(s.numGroups)/3).ceil == (Float(s.numGroups)/3).ceil - 1 && !s.lineItemView %>
			</div>
			<% open = false %>
		  <% end %>
		<% end %>
	<% end %>
	<%= '</div>' if open && !s.lineItemView %>
	<%= will_paginate @products if s.directLayout && @groupings.nil?%>
	</div>
	<%= '</div>' if s.lineItemView %>
	<% current_count = Search.count(:conditions => "session_id = #{s.id}") %>
	<div style="clear:both;"><span id="actioncount" style="display:none"><%= current_count %></span><%= '<span id="tourautostart">&nbsp;</span>' if (current_count <= 1) %></div>
