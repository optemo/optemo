<div class="cell">
	<!-- If the interesting feature should be displayed then display -->
	<% if @interestingFeatureDisplayed[countVar] == true %>
		<td>
		<% if column == 'brand'%>
			<%= product.display(column) + ' ' +product.display('model') %>
		<!-- Display Unavailable instead of Unknown-->
		<% elsif product.display(column) == 'Unknown' %>
			Unavailable 
		<% elsif product.display(column) == 'true' %>
			&#10003; 
		<% elsif product.display(column) == 'false' %>
			x
		<% else %>
			<%= product.display(column)[0..20] %>
		<% end %>			
		</td>
	<% end %>
</div>